<VirtualHost *:80>
  DocumentRoot {{ __nextcloud_document_root }}
  Protocols h2 http/1.1
  
  <Directory {{ __nextcloud_document_root }}>
    Require all granted
    AllowOverride All
    Options FollowSymLinks MultiViews

    <IfModule mod_dav.c>
      Dav off
    </IfModule>
  </Directory>
</VirtualHost>